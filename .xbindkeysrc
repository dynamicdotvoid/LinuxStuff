"doas /usr/bin/brightness-control /sys/class/backlight/gmux_backlight/brightness down 25"
  XF86MonBrightnessDown

"doas /usr/bin/brightness-control /sys/class/backlight/gmux_backlight/brightness up 25"
  XF86MonBrightnessUp

"doas /usr/bin/brightness-control /sys/devices/platform/applesmc.768/leds/smc::kbd_backlight/brightness down 25"
  XF86KbdBrightnessDown

"doas /usr/bin/brightness-control /sys/devices/platform/applesmc.768/leds/smc::kbd_backlight/brightness up 25"
  XF86KbdBrightnessUp

"doas poweroff"
  XF86PowerOff
